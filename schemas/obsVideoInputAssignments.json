    "type": "object",
    "additionalProperties": false,
    "properties": {
        "cameraCaptures": {
            "type": "array",
            "items": { "type": ["string", "null"] }
        },
        "gameCaptures": {
            "type": "array",
            "items": { "type": ["string", "null"] }
        }
    },
    "required": ["gameCaptures", "cameraCaptures"],
