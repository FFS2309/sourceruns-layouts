    "type": "object",
    "additionalProperties": false,
    "properties": {
        "messageText": { "type": "string" },
        "running": { "type": "boolean" }
    },
    "required": ["messageText", "running"],
    "default": {
        "running": false,
        "messageText": "Starting soon..."
    }
}
